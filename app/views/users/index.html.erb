<div class="row">
  <div class="col-sm-10">
    <h1>Users</h1>
    <table class="table">
      <thead>
      <tr>
        <th>ID</th>
        <th>Login</th>
        <th>Created at</th>
        <th>Updated at</th>
      </tr>
      </thead>

      <tbody>
      <% @users.each do |user| %>
        <tr>
          <td> <%= user.id %> </td>
          <td> <%= link_to user.login, user %> </td>
          <td> <%= user.created_at %> </td>
          <td> <%= user.updated_at %> </td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>